{
  "name": "konfig-manager",
  "description": "Kong API Gateway Config Manager",
  "version": "1.5.0",
  "author": "Luan Pablo Fabiano",
  "bin": {
    "konfig": "./bin/run"
  },
  "bugs": "https://github.com/LuanP/konfig-manager/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^5",
    "axios": "^0.24.0",
    "ramda": "^0.28.0",
    "rc": "^1.2.8"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^2",
    "babel-eslint": "^10.0.1",
    "chai": "^4",
    "coveralls": "^3.0.4",
    "globby": "^13",
    "mocha": "^9",
    "nock": "^13.0.2",
    "nyc": "^15",
    "sinon": "^12.0.1",
    "snazzy": "^9.0.0",
    "standard": "^16.0.0"
  },
  "engines": {
    "node": "12.13.0",
    "npm": "6.12.1"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/LuanP/konfig-manager",
  "keywords": [
    "kong",
    "kong config manager",
    "kong configuration manager",
    "kong configuration",
    "kong config"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "konfig",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "LuanP/konfig-manager",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "lint": "standard --verbose | snazzy",
    "git-hook": "npm run lint -s && npm run test -s",
    "precommit": "npm run git-hook",
    "prepush": "npm run git-hook",
    "fixcode": "standard --fix",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "version": "oclif-dev readme && git add README.md"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "test",
      "beforeEach",
      "afterEach",
      "after"
    ]
  }
}
